<h1>Admin::Genres#edit</h1>

<h2>ジャンル編集</h2>

<%= form_with model: @genre, url: admin_genre_path, method: :patch do |f| %>
<!--admin/genres#updateのpath使用-->
<label for="inputName">ジャンル</label>
<%= f.text_field:name, autofocus: true, id:"inputName" %>
<%= f.submit '変更を保存', class: "btn btn-secondary" %>

<% end %>
